<h1>Editing Book</h1>


<!-- form_for部分 -->
<%= form_for(@book) do |f|%>
	<h4>Title</h4>
	<%= f.text_field :title %>
	<h4>Body</h4>
	<%= f.text_area :body %>
	<%= f.submit 'Update Book' %>
<% end %>

<h4>
	<%= link_to "Edit", edit_book_path(@book) %> | 	<%= link_to "Back", new_book_path(@book) %>
</h4>
